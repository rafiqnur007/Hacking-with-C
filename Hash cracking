#include <stdio.h>
#include <string.h>
#include <crypt.h>

int main(int argc, char *argv[])
{
    char *hash = "$6$salt$hash";
    char *password = "password";
    char *result;

    result = crypt(password, hash);

    if (strcmp(result, hash) == 0) {
        printf("Match found!\n");
    } else {
        printf("No match found.\n");
    }
    return 0;
}
